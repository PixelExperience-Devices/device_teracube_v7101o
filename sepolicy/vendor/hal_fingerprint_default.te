# Allow fingerprint HAL to access to goodix device and files
typeattribute hal_fingerprint_default data_between_core_and_vendor_violators;
allow hal_fingerprint_default goodix_device:chr_file rw_file_perms;
allow hal_fingerprint_default goodix_data_file:dir r_dir_perms;
allow hal_fingerprint_default goodix_data_file:file rw_file_perms;

allow hal_fingerprint_default self:netlink_socket create_socket_perms_no_ioctl;
